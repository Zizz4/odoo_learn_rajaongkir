<?xml version="1.0" encoding="utf-8" ?>
<odoo>
    <record id="res_config_settings_view_form_aziz_kota_setting" model="ir.ui.view">
        <field name="name">res.config.settings.view.form</field>
        <field name="model">res.config.settings</field>
        <field name="inherit_id" ref="base_setup.res_config_settings_view_form"/>
        <field name="arch" type="xml">
           <div id="companies" position="after">
	            <h2 id="raja_ongkir_title">Raja Ongkir</h2>
	            <div class="row mt16 o_settings_container" name="raja_ongkir">
	            	<div class="col-12 col-lg-6 o_setting_box">
	                    <div class="o_setting_left_pane">
	                    </div>
	                    <div class="o_setting_right_pane">
	                        <span class="o_form_label">API Key</span>
                            <div class="text-muted">
                                Konfigurasi API Key RajaOngkir
                            </div>
                            <div class="content-group">
                                <div class="mt16 row">
                                	<label for="rajaongkir_api_key" class="col-3 o_light_label"/>
                                    <field name="rajaongkir_api_key" class="col-5"/>
                                </div>
                            </div>
	                    </div>
                    </div>
	            </div>
	        </div>
	    </field>
    </record>
</odoo>